    {
        "verb": "PUT",
        "uri": "/terms",
        "body": {
            "settings": {
                "analysis": {
                    "filter": {
                        "nGram_filter": {
                            "type": "nGram",
                            "min_gram": 2,
                            "max_gram": 20,
                            "token_chars": [
                                "letter",
                                "digit",
                                "punctuation",
                                "symbol"
                            ]
                        }
                    },
                    "analyzer": {
                        "nGram_analyzer": {
                            "type": "custom",
                            "tokenizer": "whitespace",
                            "filter": [
                                "lowercase",
                                "asciifolding",
                                "nGram_filter"
                            ]
                        },
                        "whitespace_analyzer": {
                            "type": "custom",
