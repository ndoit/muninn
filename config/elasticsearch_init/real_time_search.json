                            "filter": [
                                "lowercase",
                                "asciifolding"
                            ]
                        }
                    }
                }
            },
            "mappings": {
                "term": {
                    "_all": {
                        "index_analyzer": "nGram_analyzer",
                        "search_analyzer": "whitespace_analyzer"
                    },
                    "properties": {
                       "name": {
                            "type": "multi_field",
                            "fields": {
                                "name": {
                                    "type": "string",
                                    "store": "yes"
                                },
                                "raw": {
                                    "type": "string",
                                    "index": "not_analyzed",
                                    "analyzer": "simple",
                                    "include_in_all": true,
                                    "store": "yes"
                                }
                            }
                        },
                        "definition": {
                         "type": "multi_field",
                         "fields": {
                          "definition": {
                                    "type": "string",
                                    "store": "yes"
                            },
                            "raw": {
                                "type": "string",
                                "index": "not_analyzed",
                                 "include_in_all": true,
                                "store": "yes"
                            }
                            }
                        },
                        "source_system": {
                            "type": "string",
                            "index": "not_analyzed"
                        },
                        "possible_values": {
                            "type": "string",
                            "include_in_all": false
                        },
                        "notes": {
                            "type": "string",
                            "include_in_all": false
                        },
                        "data_sensitivity": {
                            "type": "string",
                            "include_in_all": false
                        },
                        "data_availability": {
                            "type": "string",
                            "include_in_all": false
                        },
                        "stakeholders": {

                           "properties": {
                             "id": {
                                "type": "long"
                             },                         
                             "stake": {
                                "type": "string"
                             },
                             "name": {
                                 "type": "multi_field",
                                 "fields": {
                                  "name": {
                                            "type": "string",
                                            "store": "yes"
                                    },
                                    "raw": {
                                        "type": "string",
                                        "index": "not_analyzed",
                                         "include_in_all": true,
                                        "store": "yes"
                                    }
                                 }
                           }    
                        }
                      },
                      "offices": {

